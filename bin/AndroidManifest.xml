<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.timetable"
    
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="11" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon2"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
         >
        <activity
            android:name="com.example.timetable.TimeTable"
            android:label="@string/app_name" 
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.timetable.BranchSelector"
            android:label="@string/title_activity_branch_selector" 
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name="com.example.timetable.ClassSelector"
            android:label="@string/title_activity_class_selector" 
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.timetable.BranchSelector" />
        </activity>
        <activity
            android:name="com.example.timetable.Display"
            android:label="@string/title_activity_display" 
            android:icon="@drawable/mbuttonn"
            android:screenOrientation="portrait"
            >
        </activity>
        <activity
            android:name="com.example.timetable.Settings"
            android:label="@string/title_activity_settings" 
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name="com.example.timetable.AboutUs"
            android:label="@string/title_activity_about_us"
            android:screenOrientation="portrait" >
        </activity>
       
        <activity
            android:name="com.example.timetable.Holiday"
            android:label="@string/title_activity_holiday"
            android:screenOrientation="portrait" 
          >
        </activity>
    </application>

</manifest>
